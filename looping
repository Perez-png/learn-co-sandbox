 class Bartender
  attr_accessor :name
  
  BARTENDERS = []
  
  def initialize(name)
    @name = name
    BARTENDERS << self
  end
  
  def self.all
    BARTENDERS
  end
  
  def intro
    "Hello my name is #
    {name}!"
  end
  
  def make_drink
    @cocktail_ingredients =
  []
  
    choose_liquor
    choose_mixer
    choose_garnish
    return "Here is your
  drink. It contains #
  {@cocktail_ingredients}"
    end
    
    private
    
    def choose_liquor
      @cocktail_ingredients.push("whiskey")
    end
    
    def choose_mixer
      @cocktail_ingredients.push("vermouth")
    end
    
    def choose_garnish
      @cocktail_ingredients.push("olives")
    end
    
  end
  
  phil.choose_liquor
  
  end
  
  phil.make_drink
  end
  
  


  
  class Song
  
    attr_accessor :name
    
    def initialize(name)
      @name = name
    end
  end
  
  hotline_bling =
  Song.new("Hotline Bling")
  
  thriller =
  Song.new("Thriller")
  
 
 
 
 
  class Song
  
  @@all = []
  
  attr_accessor :name
  
  def initialize(name)
    @name = name
  end
end

  
  
class Song
 
  @@all = []
 
  attr_accessor :name
 
  def initialize(name)
    @name = name
    @@all << self
  end
  
  def self.all
    @@all
  end
end

Song.all
end

